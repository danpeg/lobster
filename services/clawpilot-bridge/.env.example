# Required: Recall.ai integration
RECALL_API_KEY=__PROMPT__
# Region-specific Recall API base (examples: https://us-east-1.recall.ai, https://eu-central-1.recall.ai)
RECALL_API_BASE=https://eu-central-1.recall.ai
WEBHOOK_SECRET=__PROMPT__
WEBHOOK_BASE_URL=__PROMPT__
# Optional but strongly recommended: bearer token required on bridge control routes
BRIDGE_API_TOKEN=
# Optional: override openclaw.json path if bridge runs as a different OS user
OPENCLAW_CONFIG_PATH=

# Optional: Recall bot defaults
RECALL_LANGUAGE_CODE=en
RECALL_STT_MODE=prioritize_low_latency
OPENCLAW_AGENT_NAME=
RECALL_BOT_NAME=
RECALL_BOT_NAME_SUFFIX=Note Taker

# Optional: copilot tuning
PROACTIVITY_LEVEL=high
REACT_ON_PARTIAL=false
REACTION_COOLDOWN_MS=1100
PARTIAL_REACTION_DEBOUNCE_MS=1800
MIN_NEW_WORDS=8
PARTIAL_MIN_NEW_WORDS=6
PARTIAL_CONTEXT_WINDOW=12
FINAL_CONTEXT_WINDOW=14

# Optional: Telegram debug mirror
TELEGRAM_CHAT_ID=
DEBUG_MIRROR_TELEGRAM=false
DEBUG_MODE=false

# Optional: direct Discord delivery (recommended for discord route_target)
# Discord bot token is read from channels.discord.botToken in openclaw.json
DISCORD_DIRECT_DELIVERY=true

# Optional: speaker-gated control commands
# Example: ^dan$
CONTROL_SPEAKER_REGEX=

# Optional: server bind
PORT=3001
HOST=127.0.0.1
